{% comment %}
  Section: layout-3-way.liquid
  Purpose: 3-panel promotional layout with large video/image + 2 smaller images
  Used on: Test page (page.test.json)
  Features: Slideshow support in main panel, configurable backgrounds, responsive grid
{% endcomment %}

<style>
  .threeway-ad {
    display: grid;
    grid-template-columns: 3fr 1fr;
    grid-template-rows: 1fr 2fr;
    gap: 20px;
    max-width: 1400px;
    margin: 0 auto;
    padding: 20px;
    height: 600px;
    background-color: {{ section.settings.background_color | default: 'transparent' }};
    border-radius: {{ section.settings.background_border_radius | default: 0 }}px;
  }
  
  /* Big Container (Left) - 16:9 aspect ratio maintained through grid */
  .big-container {
    position: relative;
    border-radius: 12px;
    overflow: hidden;
    background: #f5f5f5;
    grid-row: 1 / 3;
    width: 100%;
    height: 100%;
  }
  
  .big-container__carousel {
    position: relative;
    width: 100%;
    height: 100%;
  }
  
  .big-container__slide {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
    transition: opacity 0.5s ease-in-out;
    display: flex;
    align-items: center;
    background-size: {{ section.settings.big_background_size | default: 'cover' }};
    background-position: {{ section.settings.big_background_position | default: 'center' }};
    color: white;
  }
  
  .big-container__slide.active {
    opacity: 1;
  }
  
  .big-container__content {
    padding: 40px;
    max-width: 60%;
  }
  
  .big-container__title {
    font-size: 36px;
    font-weight: bold;
    margin-bottom: 12px;
    line-height: 1.2;
    color: {{ section.settings.big_text_color | default: '#ffffff' }};
  }
  
  .big-container__subtitle {
    font-size: 18px;
    margin-bottom: 24px;
    opacity: 0.9;
    color: {{ section.settings.big_text_color | default: '#ffffff' }};
  }
  
  .big-container__button {
    background: {{ section.settings.button_color | default: '#0066cc' }};
    color: white;
    padding: 12px 24px;
    border: none;
    border-radius: 6px;
    text-decoration: none;
    display: inline-block;
    font-weight: 500;
    transition: background-color 0.3s;
  }
  
  .big-container__button:hover {
    background: {{ section.settings.button_hover_color | default: '#0052a3' }};
  }
  
  /* Small Container (Top Right) - 3:1 aspect ratio through constrained height */
  .small-container {
    position: relative;
    border-radius: 12px;
    overflow: hidden;
    background-size: {{ section.settings.small_background_size | default: 'cover' }};
    background-position: {{ section.settings.small_background_position | default: 'center' }};
    display: flex;
    align-items: center;
    justify-content: center;
    color: white;
    text-decoration: none;
    transition: transform 0.3s;
    grid-column: 2;
    grid-row: 1;
    width: 100%;
    height: 100%;
  }
  
  .small-container:hover {
    transform: translateY(-2px);
  }
  
  .small-container__content {
    padding: 20px;
    width: 100%;
    text-align: center;
  }
  
  .small-container__title {
    font-size: 18px;
    font-weight: bold;
    margin-bottom: 6px;
    color: {{ section.settings.small_text_color | default: '#ffffff' }};
  }
  
  .small-container__subtitle {
    font-size: 14px;
    color: {{ section.settings.small_text_color | default: '#ffffff' }};
    opacity: 0.9;
  }
  
  /* Medium Container (Bottom Right) - 4:3 aspect ratio through grid sizing */
  .medium-container {
    position: relative;
    border-radius: 12px;
    overflow: hidden;
    background-size: {{ section.settings.medium_background_size | default: 'cover' }};
    background-position: {{ section.settings.medium_background_position | default: 'center' }};
    display: flex;
    align-items: center;
    justify-content: flex-end;
    color: white;
    text-decoration: none;
    transition: transform 0.3s;
    grid-column: 2;
    grid-row: 2;
    width: 100%;
    height: 100%;
  }
  
  .medium-container:hover {
    transform: translateY(-2px);
  }
  
  .medium-container__content {
    padding: 30px;
    text-align: right;
    max-width: 70%;
  }
  
  .medium-container__title {
    font-size: 20px;
    font-weight: bold;
    margin-bottom: 8px;
    color: {{ section.settings.medium_text_color | default: '#ffffff' }};
  }
  
  .medium-container__subtitle {
    font-size: 16px;
    color: {{ section.settings.medium_text_color | default: '#ffffff' }};
    opacity: 0.9;
  }
  
  /* Small Container (Bottom Right) */
  .small-container {
    position: relative;
    border-radius: 12px;
    overflow: hidden;
    background-size: cover;
    background-position: center;
    display: flex;
    align-items: flex-end;
    color: white;
    text-decoration: none;
    transition: transform 0.3s;
  }
  
  .small-container:hover {
    transform: translateY(-2px);
  }
  
  .small-container__content {
    padding: 25px;
    width: 100%;
  }
  
  .small-container__title {
    font-size: 18px;
    font-weight: bold;
    margin-bottom: 6px;
    color: {{ section.settings.small_text_color | default: '#ffffff' }};
  }
  
  .small-container__subtitle {
    font-size: 14px;
    color: {{ section.settings.small_text_color | default: '#ffffff' }};
    opacity: 0.9;
  }
  
  /* Big Container Carousel Navigation */
  .big-container__nav {
    position: absolute;
    bottom: 20px;
    left: 40px;
    display: flex;
    gap: 8px;
    z-index: 10;
  }
  
  .big-container__nav-dot {
    width: 12px;
    height: 12px;
    border-radius: 50%;
    background: rgba(255, 255, 255, 0.5);
    cursor: pointer;
    transition: background-color 0.3s;
  }
  
  .big-container__nav-dot.active {
    background: white;
  }
  
  .big-container__arrows {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    background: rgba(255, 255, 255, 0.2);
    border: none;
    width: 40px;
    height: 40px;
    border-radius: 50%;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    color: white;
    transition: background-color 0.3s;
    z-index: 10;
  }
  
  .big-container__arrows:hover {
    background: rgba(255, 255, 255, 0.3);
  }
  
  .big-container__arrow-prev {
    left: 20px;
  }
  
  .big-container__arrow-next {
    right: 20px;
  }
  
  /* Big Container Pause/Play button */
  .big-container__play-pause {
    position: absolute;
    top: 20px;
    right: 20px;
    background: rgba(255, 255, 255, 0.2);
    border: none;
    width: 40px;
    height: 40px;
    border-radius: 50%;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    color: white;
    transition: background-color 0.3s;
    z-index: 10;
  }
  
  .big-container__play-pause:hover {
    background: rgba(255, 255, 255, 0.3);
  }
  
  /* Overlay for better text readability */
  .container-overlay {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.3);
    z-index: 1;
  }
  
  .big-container__content,
  .medium-container__content,
  .small-container__content {
    position: relative;
    z-index: 2;
  }
  
  /* Mobile responsive */
  @media (max-width: 768px) {
    .threeway-ad {
      grid-template-columns: 1fr;
      grid-template-rows: 250px 100px 200px;
      height: auto;
      gap: 15px;
      padding: 15px;
    }
    
    .big-container {
      grid-row: 1;
      height: 100%;
    }
    
    .big-container__content {
      padding: 25px;
      max-width: 80%;
    }
    
    .big-container__title {
      font-size: 28px;
    }
    
    .big-container__subtitle {
      font-size: 16px;
    }
    
    .small-container {
      grid-column: 1;
      grid-row: 2;
      height: 100%;
    }
    
    .small-container__content {
      padding: 15px;
    }
    
    .medium-container {
      grid-column: 1;
      grid-row: 3;
      height: 100%;
    }
    
    .medium-container__content {
      padding: 20px;
    }
  }
  
  @media (max-width: 1024px) and (min-width: 769px) {
    .threeway-ad {
      grid-template-columns: 3fr 2fr;
      height: 450px;
    }
    
    .big-container__title {
      font-size: 30px;
    }
  }
</style>

{%- style -%}
  .threeway-ad-section {
    margin-bottom: {{ section.settings.margin_bottom }}px;
    margin-top: {{ section.settings.margin_top }}px;
  }
{%- endstyle -%}

<div class="threeway-ad-section">
  <div class="threeway-ad">
    
    <!-- Big Container (Left) - Carousel -->
    <div class="big-container">
      <div class="big-container__carousel" id="big-container-carousel-{{ section.id }}">
        {%- for block in section.blocks -%}
          {%- if block.type == 'big_container_slide' -%}
            <div class="big-container__slide {% if forloop.first %}active{% endif %}" 
                 style="background-image: url('{{ block.settings.image | image_url: width: 1000 }}');">
              <div class="container-overlay"></div>
              <div class="big-container__content">
                {%- if block.settings.title != blank -%}
                  <h2 class="big-container__title">{{ block.settings.title }}</h2>
                {%- endif -%}
                {%- if block.settings.subtitle != blank -%}
                  <p class="big-container__subtitle">{{ block.settings.subtitle }}</p>
                {%- endif -%}
                {%- if block.settings.button_text != blank -%}
                  <a href="{{ block.settings.button_link }}" class="big-container__button">
                    {{ block.settings.button_text }}
                  </a>
                {%- endif -%}
              </div>
            </div>
          {%- endif -%}
        {%- endfor -%}
        
        <!-- Big Container Carousel Controls -->
        {%- if section.blocks.size > 1 -%}
          <!-- Navigation Arrows -->
          <button class="big-container__arrows big-container__arrow-prev" onclick="changeBigContainerSlide('{{ section.id }}', -1)">
            <svg width="20" height="20" viewBox="0 0 24 24" fill="currentColor">
              <path d="M15.41 7.41L14 6l-6 6 6 6 1.41-1.41L10.83 12z"/>
            </svg>
          </button>
          <button class="big-container__arrows big-container__arrow-next" onclick="changeBigContainerSlide('{{ section.id }}', 1)">
            <svg width="20" height="20" viewBox="0 0 24 24" fill="currentColor">
              <path d="M10 6L8.59 7.41 13.17 12l-4.58 4.59L10 18l6-6z"/>
            </svg>
          </button>
          
          <!-- Pause/Play Button -->
          <button class="big-container__play-pause" onclick="toggleBigContainerAutoplay('{{ section.id }}')" id="bigContainerPlayPauseBtn-{{ section.id }}">
            <svg width="20" height="20" viewBox="0 0 24 24" fill="currentColor">
              <path d="M6 19h4V5H6v14zm8-14v14h4V5h-4z"/>
            </svg>
          </button>
          
          <!-- Navigation Dots -->
          <div class="big-container__nav" id="big-container-nav-{{ section.id }}">
            {%- for block in section.blocks -%}
              {%- if block.type == 'big_container_slide' -%}
                <span class="big-container__nav-dot {% if forloop.first %}active{% endif %}" 
                      onclick="goToBigContainerSlide('{{ section.id }}', {{ forloop.index0 }})"></span>
              {%- endif -%}
            {%- endfor -%}
          </div>
        {%- endif -%}
      </div>
    </div>
    
    <!-- Small Container (Top Right) -->
    {%- if section.settings.small_container_image -%}
      <a href="{{ section.settings.small_container_link }}" class="small-container"
         style="background-image: url('{{ section.settings.small_container_image | image_url: width: 600 }}');">
        <div class="container-overlay"></div>
        <div class="small-container__content">
          {%- if section.settings.small_container_title != blank -%}
            <h3 class="small-container__title">{{ section.settings.small_container_title }}</h3>
          {%- endif -%}
          {%- if section.settings.small_container_subtitle != blank -%}
            <p class="small-container__subtitle">{{ section.settings.small_container_subtitle }}</p>
          {%- endif -%}
        </div>
      </a>
    {%- endif -%}
    
    <!-- Medium Container (Bottom Right) -->
    {%- if section.settings.medium_container_image -%}
      <a href="{{ section.settings.medium_container_link }}" class="medium-container"
         style="background-image: url('{{ section.settings.medium_container_image | image_url: width: 600 }}');">
        <div class="container-overlay"></div>
        <div class="medium-container__content">
          {%- if section.settings.medium_container_title != blank -%}
            <h3 class="medium-container__title">{{ section.settings.medium_container_title }}</h3>
          {%- endif -%}
          {%- if section.settings.medium_container_subtitle != blank -%}
            <p class="medium-container__subtitle">{{ section.settings.medium_container_subtitle }}</p>
          {%- endif -%}
        </div>
      </a>
    {%- endif -%}
    
  </div>
</div>

<script>
  let bigContainerCarousels = {};
  
  function initBigContainerCarousel(sectionId) {
    if (!bigContainerCarousels[sectionId]) {
      const slides = document.querySelectorAll(`#big-container-carousel-${sectionId} .big-container__slide`);
      bigContainerCarousels[sectionId] = {
        currentSlide: 0,
        totalSlides: slides.length,
        isPlaying: true,
        interval: null
      };
      
      // Only start autoplay if there are multiple slides
      if (bigContainerCarousels[sectionId].totalSlides > 1) {
        startBigContainerAutoplay(sectionId);
      }
    }
  }
  
  function changeBigContainerSlide(sectionId, direction) {
    const carousel = bigContainerCarousels[sectionId];
    if (!carousel || carousel.totalSlides <= 1) return;
    
    const slides = document.querySelectorAll(`#big-container-carousel-${sectionId} .big-container__slide`);
    const dots = document.querySelectorAll(`#big-container-nav-${sectionId} .big-container__nav-dot`);
    
    // Remove active class from current slide and dot
    slides[carousel.currentSlide].classList.remove('active');
    if (dots[carousel.currentSlide]) {
      dots[carousel.currentSlide].classList.remove('active');
    }
    
    // Calculate new slide index
    carousel.currentSlide += direction;
    if (carousel.currentSlide >= carousel.totalSlides) carousel.currentSlide = 0;
    if (carousel.currentSlide < 0) carousel.currentSlide = carousel.totalSlides - 1;
    
    // Add active class to new slide and dot
    slides[carousel.currentSlide].classList.add('active');
    if (dots[carousel.currentSlide]) {
      dots[carousel.currentSlide].classList.add('active');
    }
  }
  
  function goToBigContainerSlide(sectionId, slideIndex) {
    const carousel = bigContainerCarousels[sectionId];
    if (!carousel || carousel.totalSlides <= 1) return;
    
    const slides = document.querySelectorAll(`#big-container-carousel-${sectionId} .big-container__slide`);
    const dots = document.querySelectorAll(`#big-container-nav-${sectionId} .big-container__nav-dot`);
    
    // Remove active class from current slide and dot
    slides[carousel.currentSlide].classList.remove('active');
    if (dots[carousel.currentSlide]) {
      dots[carousel.currentSlide].classList.remove('active');
    }
    
    // Set new slide
    carousel.currentSlide = slideIndex;
    
    // Add active class to new slide and dot
    slides[carousel.currentSlide].classList.add('active');
    if (dots[carousel.currentSlide]) {
      dots[carousel.currentSlide].classList.add('active');
    }
  }
  
  function startBigContainerAutoplay(sectionId) {
    const carousel = bigContainerCarousels[sectionId];
    if (carousel && carousel.totalSlides > 1) {
      carousel.interval = setInterval(() => {
        changeBigContainerSlide(sectionId, 1);
      }, {{ section.settings.big_container_autoplay_speed | default: 5000 }});
    }
  }
  
  function stopBigContainerAutoplay(sectionId) {
    const carousel = bigContainerCarousels[sectionId];
    if (carousel && carousel.interval) {
      clearInterval(carousel.interval);
      carousel.interval = null;
    }
  }
  
  function toggleBigContainerAutoplay(sectionId) {
    const carousel = bigContainerCarousels[sectionId];
    if (!carousel || carousel.totalSlides <= 1) return;
    
    const playPauseBtn = document.getElementById(`bigContainerPlayPauseBtn-${sectionId}`);
    
    if (carousel.isPlaying) {
      stopBigContainerAutoplay(sectionId);
      carousel.isPlaying = false;
      if (playPauseBtn) {
        playPauseBtn.innerHTML = '<svg width="20" height="20" viewBox="0 0 24 24" fill="currentColor"><path d="M8 5v14l11-7z"/></svg>';
      }
    } else {
      startBigContainerAutoplay(sectionId);
      carousel.isPlaying = true;
      if (playPauseBtn) {
        playPauseBtn.innerHTML = '<svg width="20" height="20" viewBox="0 0 24 24" fill="currentColor"><path d="M6 19h4V5H6v14zm8-14v14h4V5h-4z"/></svg>';
      }
    }
  }
  
  // Initialize Big Container carousel when DOM is loaded
  document.addEventListener('DOMContentLoaded', function() {
    initBigContainerCarousel('{{ section.id }}');
    
    // Pause on hover for Big Container only
    const carousel = document.getElementById('big-container-carousel-{{ section.id }}');
    if (carousel && bigContainerCarousels['{{ section.id }}'] && bigContainerCarousels['{{ section.id }}'].totalSlides > 1) {
      carousel.addEventListener('mouseenter', () => stopBigContainerAutoplay('{{ section.id }}'));
      carousel.addEventListener('mouseleave', () => {
        if (bigContainerCarousels['{{ section.id }}'].isPlaying) {
          startBigContainerAutoplay('{{ section.id }}');
        }
      });
    }
  });
</script>

